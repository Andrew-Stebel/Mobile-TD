        <activity
            android:name="com.example.td.CompanyDetailsActivity"
            android:label="@string/title_activity_company_details" >
        </activity>
        <activity
            android:name="com.example.td.CompanyCompareActivity"
            android:label="@string/title_activity_company_compare" >
        </activity>
        <activity
            android:name="com.example.td.HelpActivity"
            android:label="@string/title_activity_help" >
        </activity>
        <activity
            android:name="com.example.td.AboutActivity"
            android:label="@string/title_activity_about" >
        </activity>
        <activity
            android:name="com.example.td.CategorySelectionActivity"
            android:label="@string/title_activity_category_selection" >
        </activity>
        <activity
            android:name="com.example.td.CompaniesActivity"
            android:label="@string/title_activity_companies" >
        </activity>
    </application>

</manifest>
